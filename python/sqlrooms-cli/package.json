{
  "name": "sqlrooms-cli-python",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:server": "uv run sqlrooms --ws-port 4000 --port 4173 --no-open-browser",
    "dev:ui": "pnpm --filter sqlrooms-cli-app dev -- --host --port 4174",
    "dev": "concurrently -k -n server,ui -c blue,magenta \"pnpm run dev:server\" \"pnpm run dev:ui\"",
    "build:ui": "pnpm --filter sqlrooms-cli-app build",
    "check-fix": "uv run ruff check --fix",
    "format": "uv run ruff format",
    "prerelease": "uv run pytest && uv run ruff check && uv run ruff format --check",
    "release": "uv build && uvx twine upload --skip-existing ./dist/sqlrooms*",
    "test": "uv run pytest -s --log-cli-level=DEBUG"
  }
}
